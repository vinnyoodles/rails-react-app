<!DOCTYPE html>
<html>
<head>
  <title>Rails React App</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='wrapper'>
    <%= react_component('app.Sidebar', {
      :items => ['good'],
      :itemsPluralized => ['goods'],
      :filtered => false,
      :keys => ['id'],
      :types => ['string'],
    }.to_json) %>
    <%= yield %>
  </div>
</body>
</html>
